<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.AppointmentMenuController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="66.0" prefWidth="250.0" text="Appointment">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <VBox layoutX="14.0" layoutY="75.0" prefHeight="251.0" prefWidth="100.0" spacing="20.0">
            <children>
                <Label text="Appointment ID" />
                <Label text="Title" />
                <Label text="Description" />
                <Label text="Location" />
            </children>
        </VBox>
        <VBox layoutX="106.0" layoutY="75.0" prefHeight="191.0" prefWidth="125.0" spacing="12.0">
            <children>
                <TextField fx:id="aidField" disable="true" editable="false" />
                <TextField fx:id="appTitleField" />
                <TextField fx:id="aDescField" />
                <TextField fx:id="locationField" />
                <ComboBox fx:id="appContactBox" prefWidth="150.0" promptText="Contact" />
                <ComboBox fx:id="typeBox" prefWidth="150.0" promptText="Type" />
                <ComboBox fx:id="appSDateTime" prefWidth="150.0" promptText="Start Date/Time" />
                <ComboBox fx:id="appEDateTime" prefWidth="150.0" promptText="End Date/Time" />
                <TextField fx:id="appCustIdText" disable="true" editable="false" promptText="Customer ID" />
                <TextField fx:id="appUserIdText" disable="true" editable="false" promptText="User ID" />
            </children>
        </VBox>
        <TableView layoutX="246.0" layoutY="63.0" prefHeight="334.0" prefWidth="562.0">
            <columns>
                <TableColumn prefWidth="39.0" text="ID" />
                <TableColumn prefWidth="53.0" text="Title" />
                <TableColumn prefWidth="82.0" text="Description" />
                <TableColumn prefWidth="70.0" text="Location" />
                <TableColumn prefWidth="65.0" text="Contact" />
                <TableColumn prefWidth="52.0" text="Type" />
                <TableColumn prefWidth="51.0" text="Start" />
                <TableColumn prefWidth="57.0" text="End" />
                <TableColumn prefWidth="47.0" text="CustID" />
                <TableColumn prefWidth="43.0" text="UserID" />
            </columns>
        </TableView>
        <Button layoutX="172.0" layoutY="460.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="118.0" text="Add" />
        <Button layoutX="645.0" layoutY="460.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="125.0" text="Back" />
        <Button layoutX="332.0" layoutY="460.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="118.0" text="Update" />
        <Button layoutX="482.0" layoutY="460.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="125.0" text="Delete" />
    </children>

</AnchorPane>
